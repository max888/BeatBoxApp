<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @song.name %>
</p>

<p>
  <b>Genre:</b>
  <%= @song.genre %>
</p>

<audio controls>
  <source src="<%=@song.file %>" type="audio/mpeg">
